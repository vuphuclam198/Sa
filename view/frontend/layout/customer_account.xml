<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="sidebar.main.account_nav">
            <referenceBlock name="customer_account_navigation">
                <!-- <block class="Magento\Framework\View\Element\Template" name="commission.title" template="AHT_Sa::commission.phtml" />         -->
                <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-commission-link">
                    <arguments>
                        <argument name="label" xsi:type="string" translate="true">Commission</argument>
                        <argument name="path" xsi:type="string">sale_sa/Commission/index</argument>
                        <argument name="sortOrder" xsi:type="number">200</argument>
                    </arguments>
                </block>
            </referenceBlock>
        </referenceBlock>
    </body>
</page>